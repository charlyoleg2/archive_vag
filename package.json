{
	"name": "archive_vag",
	"version": "0.0.1",
	"description": "top-repo that points to the other archive repositories",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MYNAME/archive_vag.git"
	},
	"homepage": "https://github.com/MYNAME/archive_vag#readme",
	"author": "MYNAME",
	"license": "ISC",
	"keywords": [
		"archive",
		"vag"
	],
	"type": "module",
	"main": "index.js",
	"scripts": {
		"vag-list": "vag --importYaml=archive_repos.yml list",
		"vag-clone": "vag --importYaml=archive_repos.yml clone",
		"vag-status": "vag --importYaml=archive_repos.yml --only_configured status",
		"vag-diff": "vag --importYaml=archive_repos.yml --only_configured diff",
		"vag-branch": "vag branch",
		"vag-remote": "vag remote",
		"vag-pull": "vag pull",
		"vag-push": "vag push",
		"vag-verify": "vag verify --importYaml=archive_repos.yml",
		"vag-versions": "vag versions",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"devDependencies": {
		"vag_tools": "^0.0.7"
	}
}
